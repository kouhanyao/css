table,body,tr,td,p,span,ul,li,img{
    padding:0;
    margin:0;
}
#wrap{
    max-width: 1920px;
    width:100%;
    font-family: "微软雅黑", sans-serif;
    line-height: 100%;

}
/*第一部分*/
header{
    padding:0.625% 0 0.625% 18.75%;
    color:green;
    position: relative;
    font-size: 0.875em;
	background:yellow;

}
header table{
    position: absolute;
    left:0;
    top:15.3%;
    width:10%;
    border-collapse:collapse;
    text-align: right;
	background:red;
}

/*第二部分*/
#logo{
    padding:1.4% 0 1.4% 18.75%;
    position: relative;
    color:white;
    background: #26a26f;
	width:100%;
}
#logo p{
    display: inline;
    margin-left:0%;
    position: absolute;
    top:42%;
    width:100%;
	background:yellow;
}
#logo span{
    margin-left:3.3%;
}
/*第三部分*/
#banner img{
    width:100%;
    display: block;
}
/*第四部分*/
.company ul li:hover{

    list-style-image:url(images8/yellow-circle.png);
}
#content{
    overflow:hidden;
    line-height: 150%;
    background: #fafafa;
	padding-bottom:12%;
	width:100%;
}
#content .title{
    padding:1.56% 0 1.56% 18.75%;
}
#content .title span{
    color:green;
}
.company{
    width:15.9%;
    margin-left:18.75%;
    /*text-align: center;*/
}
.company,.content{
    float:left;
    background: #ffffff;
}
.company p{
    font-weight: bold;
    padding:6.6% 0 22% 0;
    text-align: center;
}
.company ul li{
    list-style-image:url(images8/white-circle.png);
    /*list-style-type: square;*/
    list-style-position: inside;
    padding-left:38.4%;
    margin-bottom: 12%;
    color:green;
    font-size:0.875em;
}
.company ul .click{
    list-style-image:url(images8/yellow-circle.png);
}
.content {
    width:45.5%;
    margin-left:1.3%;
    overflow: hidden;
    font-size:0.875em;
    padding:1.2% 0 7% 0;
}
.content img{
    float:left;
    margin:6% 0 0 10%;
}
.content p{
    margin-bottom:4.5%;
    color:#999999;
    text-indent: 4%;
    padding:0 8% 0 8%;
}
.content .one{
    padding-left:33%;
    margin-bottom: 5.7%;
}
/*第五部分*/
footer{
    background: #26a26f;
    color:white;
    overflow: hidden;
    line-height: 150%;
}
footer .footerOne div{
    float: left;
    padding:1.875% 0 1% 0;
}
footer .footerOne .one{
    margin-left:18.75%;
}
footer .footerOne .two{
    margin-left:5%;
 }
footer .footerOne .three{
    margin-left:17.5%;
}
footer .footerTwo{
    clear:both;
    text-align: center;
    border-top: 1px solid #32c086;
}